<script>
document.addEventListener('DOMContentLoaded', function() {
	var favoriteButton = document.getElementById('favorite-btn');
	var loginModal = document.getElementById('login-modal');
	/* var loginLink = document.getElementById('login-link'); */
	var closeModal = document.getElementsByClassName('close')[0];

	function updateFavoriteButtonText(isFavorite) {
		// Update the button text
		if (isFavorite) {
			favoriteButton.innerHTML = '<i class="fa fa-heart wishlist-icon remove"></i>';
		} else {
			favoriteButton.innerHTML = '<i class="fa fa-heart wishlist-icon"></i>';
		}
	}

	function handleAjaxResponse(xhr) {
		// Handle the AJAX response
		if (xhr.status === 200) {
			var response = JSON.parse(xhr.responseText);

			if (response.success) {
				updateFavoriteButtonText(response.isFavorite);
			} else {
				console.log('Request failed. Returned error: ' + response.message);
			}
		} else {
			console.log('Request failed. Returned status of ' + xhr.status);
		}
	}

	function handleLogin() {
		// Redirect the user to the login page
		window.location.href = 'https://stmify.com/wp-login.php';
	}

	favoriteButton.addEventListener('click', function(event) {
		event.preventDefault(); // Prevent the default link behavior

		var isLoggedIn = false;

		if (isLoggedIn) {
			// Make an AJAX request to the server to handle the favorite action
			var xhr = new XMLHttpRequest();
			xhr.open('POST', 'https://stmify.com/wp-admin/admin-ajax.php', true);
			xhr.setRequestHeader('Content-Type', 'application/x-www-form-urlencoded; charset=UTF-8');
			xhr.send('action=add_to_favorite&post_id=3066');

			xhr.onload = function() {
				handleAjaxResponse(xhr);
			};
		} else {
			// Display the login modal
			loginModal.style.display = 'block';
		}
	});

	/* loginLink.addEventListener('click', function(event) {
		event.preventDefault();
		handleLogin();
	}); */

	closeModal.addEventListener('click', function() {
		loginModal.style.display = 'none';
	});

	window.addEventListener('click', function(event) {
		if (event.target === loginModal) {
			loginModal.style.display = 'none';
		}
	});

	document.addEventListener('keydown', function(event) {
		if (event.key === 'Escape' && loginModal.style.display === 'block') {
			loginModal.style.display = 'none';
		}
	});

	// Check the initial favorite status of the post
	var xhr = new XMLHttpRequest();
	xhr.open('POST', 'https://stmify.com/wp-admin/admin-ajax.php', true);
	xhr.setRequestHeader('Content-Type', 'application/x-www-form-urlencoded; charset=UTF-8');
	xhr.send('action=check_favorite_status&post_id=3066');

	xhr.onload = function() {
		handleAjaxResponse(xhr);
	};
});

</script>        <div class="content-wraper">
