<script>
// Function to copy the iframe code to clipboard
function copyToClipboard(sectionId) {
    var textarea = document.querySelector(`#${sectionId} textarea`);
    textarea.select();
    document.execCommand('copy');
    
    // Get the button and icons inside the button
    var button = document.querySelector(`#${sectionId} .copy-btn`);
    var copyIcon = button.querySelector('.copy-icon');
    var checkIcon = button.querySelector('.check-icon');
    
    // Hide copy icon and show check icon
    copyIcon.style.display = 'none';
    checkIcon.style.display = 'inline'; 
    
    // Alert to indicate successful copy
    alert('Iframe code copied to clipboard!');
    
    // Optional: Reset the icons back after a delay (e.g., 2 seconds)
    setTimeout(function() {
        copyIcon.style.display = 'inline';
        checkIcon.style.display = 'none';
    }, 2000); // Reset after 2 seconds
}

</script>
