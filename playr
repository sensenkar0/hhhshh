<script data-cfasync="false" src="/cdn-cgi/scripts/5c5dd728/cloudflare-static/email-decode.min.js"></script><script>
const chatToggle = document.getElementById('chat-toggle');
const chatContainer = document.getElementById('chatbox-container');
const chatbox = document.getElementById('chatbox');
const usernameInput = document.getElementById('username');
const messageInput = document.getElementById('message');

// Show or hide the chatbox when the icon is clicked
chatToggle.addEventListener('click', () => {
  const isHidden = chatContainer.style.display === 'none' || chatContainer.style.display === '';

  // Toggle chatbox visibility
  chatContainer.style.display = isHidden ? 'block' : 'none';

  // Toggle icon
  if (isHidden) {
    icon.classList.remove('fab', 'fa-facebook-messenger');
    icon.classList.add('fa', 'fa-times');
  } else {
    icon.classList.remove('fa', 'fa-times');
    icon.classList.add('fab', 'fa-facebook-messenger');
  }
});

// Check if username is stored in localStorage
if (localStorage.getItem('username')) {
  usernameInput.value = localStorage.getItem('username');
  usernameInput.disabled = true; // Disable the username input after the first entry
}

// Variable to track if the user is manually scrolling
let isUserScrolling = false;

// Check if the user is at the bottom of the chatbox
function isAtBottom() {
  return chatbox.scrollHeight - chatbox.scrollTop === chatbox.clientHeight;
}

// Event listener to detect user scroll
chatbox.addEventListener('scroll', () => {
  // If the user scrolls up, stop auto-scrolling
  if (chatbox.scrollTop < chatbox.scrollHeight - chatbox.clientHeight - 10) {
    isUserScrolling = true;
  } else {
    isUserScrolling = false;
  }
});

// Fetch chat logs from the server
function fetchChats() {
  const xhr = new XMLHttpRequest();
  xhr.open('POST', 'https://stmify.com/wp-content/themes/stmify/assets/chatbox.php', true);
  xhr.setRequestHeader('Content-Type', 'application/x-www-form-urlencoded');
  xhr.onload = function () {
    if (xhr.status === 200) {
      const previousScrollHeight = chatbox.scrollHeight; // Get the scroll height before updating content

      chatbox.innerHTML = xhr.responseText; // Update the chatbox content

      // Scroll to the bottom only if the user is at the bottom or there is new content at the bottom
      if (!isUserScrolling || chatbox.scrollHeight > previousScrollHeight) {
        chatbox.scrollTop = chatbox.scrollHeight;
      }
    }
  };
  xhr.send('action=fetch');
}

// Send message to the server
function sendMessage() {
  let username = usernameInput.value.trim();
  let message = messageInput.value.trim();

  if (username === '' || message === '') return;

  // Store username in localStorage if it's not already saved
  if (!localStorage.getItem('username')) {
    localStorage.setItem('username', username);
  }

  const xhr = new XMLHttpRequest();
  xhr.open('POST', 'https://stmify.com/wp-content/themes/stmify/assets/chatbox.php', true);
  xhr.setRequestHeader('Content-Type', 'application/x-www-form-urlencoded');
  xhr.onload = function () {
    if (xhr.status === 200) {
      messageInput.value = ''; // Clear the message input
      fetchChats(); // Refresh the chatbox with the new message
    }
  };
  xhr.send('action=send&username=' + encodeURIComponent(username) + '&message=' + encodeURIComponent(message));
}

// Auto-refresh every second
setInterval(fetchChats, 1000);

// Initial fetch of chat messages and scroll to the bottom
setTimeout(() => {
  fetchChats();
  chatbox.scrollTop = chatbox.scrollHeight; // Scroll to the bottom immediately
}, 100);
</script> 
			
